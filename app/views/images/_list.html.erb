<table  class="table">
  <tr>
    <th>Id</th>
    <th>Url Path</th>
    <th colspan="3"></th>
  </tr>
  <% for image in @images %>
    <tr>
      <td><%= image.id.to_s %></td>
      <td><%= image.url_path %></td>
      <td><%= link_to "Show", image %></td>
      <td><%= link_to "Edit", edit_image_path(image) %></td>
      <td><%= link_to "Destroy", image, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>